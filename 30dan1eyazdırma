using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _30to1
{
    internal class Program
    {
        static void Main(string[] args)
        {

            for (int i = 30; i >= 1 ; i--)
            {
                Console.WriteLine(i);
            }
            Console.ReadLine();
        }
    }
}
